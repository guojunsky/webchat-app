.types-title{
   margin: 50rpx 0 16rpx;
   display: flex;
   justify-content: center;
   position: relative;
   background: transparent;
}
.t-name{
  color: #7a45e5;
  font-size: 28rpx;
  display: block;
  padding: 0 20rpx;
  position: relative
}
.t-name:before{
    content: ' ';
    display: block;
    height: 1px;
    width: 50rpx;
    position: absolute;
    left: -50rpx;
    top: 50%;
    margin-top: -1px;
    z-index: 5;
    background: #7a45e5;
    transform: scaleY(.5);
}

.t-name:after{
    content: ' ';
    display: block;
    height: 1px;
    width: 50rpx;
    position: absolute;
    left: 100%;
    top: 50%;
    margin-top: -1px;
    z-index: 5;
    background: #7a45e5;
    transform: scaleY(.5);
}
