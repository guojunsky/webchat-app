.price-top{
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50px;
  background-image: -webkit-linear-gradient(left,#7A45E5,#EF4DB7);
  text-align: center;
  font-size: 13px;
  line-height: 13px;
  color: #FFF;
  z-index: 1;
  transform: scale3d(.1,.1,.1);
  opacity: 0;
  transition: transform .5s ease-out,-webkit-transform .5s ease-out,-moz-transform .5s ease-out;
}
.t-show{
    animation: breathe 5s infinite ease-in-out;
    animation-delay: 6s;
    opacity: .9;
    transform: scale3d(1,1,1);
}
